                                        @*<p class="text-muted mb-0">@Session["Firstname"]</p>*@
                                   </div>
                              </div>
                              <hr>
                              <div class="row">
                                   <div class="col-sm-3">
                                        <p class="mb-0">Lastname</p>
                                   </div>
                                   <div class="col-sm-9">
                                        <p class="text-muted mb-0">@Html.DisplayFor(modelitem => modelitem.Lastname)</p>
                                        @*<p class="text-muted mb-0">@Session["Lastname"]</p>*@
                                   </div>
                              </div>
                              <hr>
                              <div class="row">
                                   <div class="col-sm-3">
                                        <p class="mb-0">Email</p>
                                   </div>
                                   <div class="col-sm-9">
                                        <p class="text-muted mb-0">@Html.DisplayFor(modelitem => modelitem.Email)</p>
                                        @*<p class="text-muted mb-0">@Session["Email"]</p>*@
                                   </div>
                              </div>
