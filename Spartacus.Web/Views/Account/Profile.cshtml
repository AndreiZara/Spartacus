                              }
                         </div>
                    </div>
                    <div class="card mb-4 mb-lg-0">
                         
                    </div>
               </div>

               <div class="col-lg-8">

                    <div class="card mb-4">
                         <div class="card-body">
                              <div class="row">
                                   <div class="col-sm-3">
                                        <p class="mb-0">Username</p>
                                   </div>
                                   <div class="col-sm-9">
                                        <p class="text-muted mb-0">@Html.DisplayFor(modelitem => modelitem.Username)</p>
