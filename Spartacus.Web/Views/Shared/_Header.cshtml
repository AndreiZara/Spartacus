               <li class="nav-item"><a class="nav-link" href=@Url.Action("About", "Home")>About us</a></li>
          </ul>
          <ul class="navbar-nav">

               
               @if (false)
               {
                    <li class="nav-item"><a class="nav-link" href=@Url.Action("Login", "Account")>Login</a></li>
                    <li class="vr text-white opacity-100 px-0"></li>
                    <li class="nav-item pe-0"><a class="nav-link" href=@Url.Action("Join", "Account")>Join</a></li>
               }
               else
               {
               <li class="nav-item"><a class="nav-link" href=@Url.Action("Login", "Account")>@Session["Username"]</a></li>
               }

          </ul>
     </div>
</div>
